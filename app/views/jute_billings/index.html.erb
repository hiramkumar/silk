<h1>Listing Jute Billings</h1>
<table class="wide-table" id="gradient-style" >
  <tr>
  	<th align = 'left'><%= Translate.trans("Vendor Name",session[:language])%></th>
  	<th align = 'left'><%= Translate.trans("Date",session[:language])%></th>
    <th align = 'left'><%= Translate.trans("Received Qty",session[:language])%></th>
    <th align = 'left'><%= Translate.trans("Jute Type",session[:language])%></th>
    <th align = 'left'><%= Translate.trans("Rate (₹)",session[:language])%></th>
    <th align = 'left'><%= Translate.trans("Discount (₹)",session[:language])%></th>
    <th align = 'left'><%= Translate.trans("Advance Amount (₹)",session[:language])%></th>
    <th align = 'left'><%= Translate.trans("Amount To Vendor (₹)",session[:language])%></th>
    <th align = 'left'><%= Translate.trans("Return Qty",session[:language])%></th>
    <th align = 'left'><%= Translate.trans("Remarks",session[:language])%></th>
	<th align = 'center' colspan = '3'><%= Translate.trans("Options",session[:language])%></th>
  </tr>
<% for jute_billing in @jute_billings %>
  <tr>
  	<% vendor =  Vendor.find_by_id(jute_billing.vendor_id) %>
  	<td><%= h vendor.name%></td>
    <td><%=h jute_billing.date.strftime("%d/%m/%y") %></td>
    <td><%=h jute_billing.received_qty %></td>
    <td><%=h jute_billing.description %></td>
    <td><%=h jute_billing.rate %></td>
    <td><%=h jute_billing.discount %></td>
    <td><%=h jute_billing.adv_amt %></td>
    <td><%=h jute_billing.amt_to_vendor %></td>
    <td><%=h jute_billing.return_qty %></td>
    <td><%=h jute_billing.remarks %></td>
    <td><%= link_to image_tag( 'show.png'), jute_billing %></td>
    <td><%= link_to image_tag( 'edit.png'), edit_jute_billing_path(jute_billing) %></td>
    <td><%= link_to image_tag( 'delete.png'), jute_billing, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

